<form class="flexcol {{cssClass}} {{actor.type}}" autocomplete="off">
  {{!-- Sheet Header --}}
  {{> "systems/mw-destiny/templates/actor/parts/header.hbs"}}

  {{!-- Sheet Tab Navigation --}}
  <nav class="sheet-tabs tabs" data-group="primary">
    {{!-- Default tab is defined in pc-sheet.mjs --}}
    <a class="item" data-tab="main">{{localize "MWDESTINY.tabs.main"}}</a>
    <a class="item" data-tab="history">{{localize "MWDESTINY.tabs.history"}}</a>
  </nav>

  {{!-- Sheet Body --}}
  <section class="sheet-body">
    {{!-- Stats Tab --}}
    <div class="tab main" data-group="primary" data-tab="main">
      {{> "systems/mw-destiny/templates/actor/parts/tab-main.hbs"}}
    </div>

    {{!-- History Tab --}}
    <div class="tab history" data-group="primary" data-tab="history">
      {{> "systems/mw-destiny/templates/actor/parts/tab-history.hbs"}}
    </div>

  </section>
</form>
